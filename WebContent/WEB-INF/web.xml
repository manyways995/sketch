<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
xmlns="http://xmlns.jcp.org/xml/ns/javaee" 
xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee 
http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" version="3.1">

  <servlet>
    <servlet-name>read</servlet-name>
    <servlet-class>cc.blog.ReadServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>read</servlet-name>
    <url-pattern>/reads.do</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>post</servlet-name>
    <servlet-class>cc.blog.PostServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>post</servlet-name>
    <url-pattern>/posts.do</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>backup</servlet-name>
    <servlet-class></servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>backup</servlet-name>
    <url-pattern></url-pattern>
  </servlet-mapping>



</web-app>